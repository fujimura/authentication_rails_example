<h2>user names:</h2>
<ul>
  <% @users.each do |user| %>
    <li><%= user.username %></li>
  <% end %>
</ul>

<p>Password is <%= link_to 'here', 'https://github.com/fujimura/authentication_rails_example/blob/master/db/seeds.rb' %>.</p>

<%= form_tag session_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <%= label_tag :password %>
  <%= text_field_tag :password %>
  <%= submit_tag 'Log in' %>
<% end %>
